page {
  background-color: #FFFBF2;
  padding: 30rpx 30rpx 0 30rpx;
  box-sizing: border-box;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.top {
  height: 200rpx;
  width: 100%;
}

.next2 {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  box-sizing: border-box;
  padding: 40rpx 0 0 0;
  background-color: #ffffff;
  box-shadow: 0px 4px 10px 0px rgba(9, 36, 66, 0.04);
}

.sub-container {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.d1 {
  color: #1f2025;
  font-size: 28rpx;
  margin-bottom: 50rpx;
  line-height: 42rpx;
  text-indent: 2em;
}

.d2 {
  color: #1f2025;
  font-size: 34rpx;
}

.next-inner {
  display: flex;
  width: 100%;
  flex-direction: column;
  align-items: center;
  position: relative;
  margin-bottom: 46rpx;
}

.next-inner text {
  font-size: 32rpx;
  z-index: 99;
  font-weight: bold;
}

.block {
  width: 608rpx;
  height: 20rpx;
  background-color: #dfedff;
  position: absolute;
  top: 30rpx;
}

.block2 {
  width: 320rpx;
  height: 20rpx;
  background-color: #dfedff;
  position: absolute;
  top: 30rpx;
}

.next3 {
  width: 100%;
  margin-top: 20rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  padding: 40rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.next3 .one-row:first-child {
  margin-bottom: 32rpx;
  border-bottom: 3rpx solid #f3f3f3;
  padding-bottom: 32rpx;
}

.one-row {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.one-row image {
  width: 600rpx;
  height: 600rpx;
  display: inline-block;
  overflow: hidden;
}

.right-side {
  padding-left: 14rpx;
  border-left: 3rpx dashed #3963bc;
}

.left-side {
  display: flex;
  flex-direction: column;
}

.left-side text:first-child {
  color: #999999;
  font-size: 24rpx;
  margin-bottom: 10rpx;
}

.left-side text:last-child {
  color: #3963bc;
  font-size: 36rpx;
}

.btn {
  background-color: #dfedff;
  color: #3963bc;
  width: 50rpx;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 20rpx;
}

.next4 {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 10rpx;
}

.logo {
  width: 100%;
  height: 360rpx;
}
